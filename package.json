{
  "name": "rn_car_app",
  "version": "0.0.1",
  "appVersion": "8.15",
  "private": true,
  "scripts": {
    "patch": "crn-cli run-patch",
    "start": "crn-cli start --port 5389",
    "android": "crn-cli run-android --app-version 8.17 --port 5389",
    "ios": "crn-cli run-ios --app-version 8.17 --port 5389",
    "start:trip": "crn-cli start --port 5388",
    "android:trip": "crn-cli run-android --appid 37 --app-version 7.4.0 --simulator 'iPhone 8 Plus' --port 5388",
    "ios:trip": "crn-cli run-ios --appid 37 --app-version 7.4.0 --simulator 'iPhone 8 Plus' --reset --port 5388",
    "lint": "npm run lint:js",
    "lint:js": "eslint . --ignore-path .eslintignore --ext .js,.ts,.tsx",
    "lint:fix": "npm run lint:js:fix",
    "lint:js:staged": "eslint . --ignore-path .eslintignore",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:js:fix:staged": "eslint . --ignore-path .eslintignore --fix",
    "ts:build": "tsc",
    "ts:watch": "tsc -w",
    "build:version": "sh ./scripts/BuildVersion.sh",
    "precommit": "lint-staged",
    "test": "jest --coverage",
    "test:fix": "npm run test -- -u",
    "clean": "watchman watch-del-all && rm -rf package-lock.json && rm -rf node_modules && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/haste-map-* && npm run install:ctrip",
    "product:ts": "rm -rf node_modules && rm -rf  package-lock.json && npm install --production",
    "install:ctrip": "npm install --registry=http://registry.npm.release.ctripcorp.com",
    "install": "tsc",
    "bbk:link": "./scripts/bbkLocalLink.sh",
    "bbk:update": "node ./scripts/updateBbkVersion.js && ./scripts/bbkInstall.sh"
  },
  "peerDependencies": {
    "@ctrip/bbk-tokens": "^0.0.34",
    "@ctrip/bbk-theming": "^0.0.35",
    "@ctrip/bbk-utils": "^0.0.39"
